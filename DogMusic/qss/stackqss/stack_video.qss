
QTabWidget
{
    background-color: white;
}

/*Tab基本的设置*/
QTabBar::tab
{
    background: white;
    border-bottom: 3px solid rgb(255,255,255);

    font-family: "微软雅黑";
    font-size: 15pt;
    font-weight: 200;
    margin-left: 5px;
    margin-right: 5px;
    margin-top: 10px;
}

/*Tab选中时*/
QTabBar::tab:selected
{
    border-bottom: 3px solid rgb(235,65,65);
    color: rgb(55,55,55);
    font-size: 17pt;
    font-weight: 590;
}
