

QLabel#localLabel
{
    font-size: 23px;
    font-weight: 550;
    border: transparent;
}

QLabel#numberlabel
{
    font-size: 17px;
    color: rgb(180,180,180);
    border: transparent;
}

QPushButton#selectButton
{
    color: #50a7ff;
    font-size: 17px;
    border: transparent;
}

QPushButton#selectButton:hover
{
    color: blue;
    font-size: 17px;
    border: transparent;
}

QPushButton#playallBtn
{
    background-color: rgb(255,80,80);
    border: transparent;
    color: white;
    border-radius: 17px;
    font-size: 17px;
}

QPushButton#playallBtn:hover
{
    background-color: rgb(220,72,57);
    border: transparent;
    color: white;
    border-radius: 17px;
    font-size: 17px;
}

QPushButton#matchMusicbtn
{
    background-color: white;
    border: 1px solid #dcdcdc;
    color: black;
    border-radius: 17px;
    font-size: 17px;
}

QPushButton#matchMusicbtn:hover
{
    background-color: #f3f3f3;
    border: 1px solid #dcdcdc;
    color: black;
    border-radius: 17px;
}

QLineEdit#localSearch
{
    background-color: #f3f3f3;
    color: #dcdcdc;
    border: none;
    border-radius: 12px;
    padding-left: 5px;
}

/*Tab基本的设置*/
QTabBar#musicInfo_TabBar:tab
{
    background: white;
    border-bottom: 3px solid rgb(255,255,255);

    font-family: "微软雅黑";
    font-weight: 200;
    margin-left: 5px;
    margin-right: 5px;
    margin-top: 10px;

    font-size: 17px;
    min-height: 25px;
    min-width: 50px;
}

/*Tab选中时*/
QTabBar#musicInfo_TabBar:tab:selected
{
    border-bottom: 3px solid rgb(235,65,65);
    color: rgb(55,55,55);

    font-weight: 550;
    color: black;
    min-height: 25px;
    min-width: 50px;
}




